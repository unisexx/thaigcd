<?php
class Public_Controller extends Master_Controller
{
	
	function __construct()
	{
		parent::__construct();
		$this->template->title('สำนักโรคติดต่อทั่วไป :: Bureau of General Communicable Diseases Department of Disease Control MOPH, Thailand');
		$this->template->set_theme('thaigcd');
    	$this->template->set_layout('layout');
		// Set js
		$this->template->append_metadata(js_notify());
		
	}
	
}
?>
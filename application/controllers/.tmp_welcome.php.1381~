<?php

class Welcome extends Public_Controller {

	function __construct()
	{
		parent::__construct();	
	}
	
	function index()
	{	
		$users = new User();
		$users->get_paged(@$_GET['page'], 3);
		$data['posts'] = $users;
		$this->template->build('welcome',$data);
	}
}

/* End of file welcome.php */
/* Location: ./application/controllers/welcome.php */